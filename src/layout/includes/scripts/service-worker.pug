script.
    if ("serviceWorker" in navigator) {
        if (navigator.serviceWorker.controller) {
            console.log("[TTWOEE] active service worker found, no need to register");
        } else {
            navigator.serviceWorker
            .register("scripts/ttwoee-sw.js", {
                scope: "./"
            })
            .then(function (reg) {
                console.log("[TTWOEE] Service worker has been registered for scope: " + reg.scope);
            });
        }
    }

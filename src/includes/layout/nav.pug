nav
    ul
        //- include hamburger.pug
        li.nav-header
            a(href='#guide') ☢️ TTW:OEE
        - for(i=0;i<sections.length;i++)
            - const section = sections[i]
            li
                a(href=section[0]) #{section[1]}

                - if(mods[i].length > 0)
                    ul
                        - for(j=0;j<mods[i].length;j++)
                            li
                                a(href=mods[i][j][0]) #{mods[i][j][1]}
